<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gamecollector" version="0.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <icon density="ldpi" src="www/img/logo.png" />
    <icon density="mdpi" src="www/img/logo.png" />
    <icon density="hdpi" src="www/img/logo.png" />
    <icon density="xhdpi" src="www/img/logo.png" />
    <name>Hadougame</name>
    <description>
        Organize sua coleção de jogos de Super Nintendo de maneira fácil e rápida.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Hadougame Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
